# Laravel Environment untuk InfinityFree Hosting
# Copy file ini menjadi .env saat deploy

APP_NAME="Rafael Portfolio"
APP_ENV=production
APP_KEY=base64:l7wX8BrVWGr5ECXyJv3wn3vRximVBo8JzCSKBRPjNcw=
APP_DEBUG=false
APP_URL=https://rafael-portofolio.free.nf

# LOGGING
LOG_CHANNEL=stack
LOG_LEVEL=error

# DATABASE - SQLITE (File-based, tidak perlu MySQL)
DB_CONNECTION=sqlite
DB_DATABASE=database/database.sqlite
DB_FOREIGN_KEYS=true

# CACHE & SESSION (File-based untuk shared hosting)
CACHE_DRIVER=file
SESSION_DRIVER=file
SESSION_LIFETIME=120

# QUEUE (Sync untuk shared hosting)
QUEUE_CONNECTION=sync

# MAIL (Biarkan default/null jika tidak digunakan)
MAIL_MAILER=null
MAIL_HOST=null
MAIL_PORT=null
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME=null

# FILESYSTEM
FILESYSTEM_DISK=local
