{
  "framework": "nitro",
  "functions": {
    "api/index.php": {
      "runtime": "php@8.2"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://your-project.vercel.app"
  }
}

